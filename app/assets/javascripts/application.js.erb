$(function() {
  var faye = new Faye.Client("http://<%= Rails.configuration.faye_host %>:<%= Rails.configuration.faye_port %>/faye");
  faye.subscribe('/card/new', function (data) {
    eval(data);
  });
  faye.subscribe('/card/update', function (data) {
    eval(data);
  });
  faye.subscribe('/ticket/new', function (data) {
    eval(data);
  });
  faye.subscribe('/ticket/update', function (data) {
    eval(data);
  });
});
